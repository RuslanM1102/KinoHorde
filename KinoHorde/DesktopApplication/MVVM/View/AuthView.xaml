<Page
    x:Class="DesktopApplication.MVVM.View.AuthView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DesktopApplication.MVVM.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="AuthView"
    d:DesignHeight="690"
    d:DesignWidth="1266"
    mc:Ignorable="d">

    <Grid Background="{DynamicResource Secondary}">
        <Button
            x:Name="buttonaaaa"
            Width="300"
            Height="100"
            VerticalAlignment="Center"
            Command="{Binding AuthCommand}"
            Content="Войти через Discord"
            FontSize="30"
            Style="{DynamicResource SearchButton}" />
    </Grid>
</Page>
